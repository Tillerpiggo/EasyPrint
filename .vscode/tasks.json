{
	"version": "2.0.0",
	"tasks": [
	{
		"label" : "npm watch",
		"type": "npm",
		"script": "watch",
		"problemMatcher": "$tsc-watch",
		"isBackground": true,
		"presentation": {
		"reveal": "never"
	},
		"group": {
		"kind": "build",
		"isDefault": true
		}
	},
	{
		"label": "npm run compile",
		"type": "shell",
		"command": "npm run compile",
		"group": {
			"kind": "build",
			"isDefault": true
		},
		"presentation": {
			"echo": true,
			"reveal": "always",
			"focus": false,
			"panel": "shared",
			"showReuseMessage": true,
			"clear": false
		},
		"problemMatcher": []
	}
	]
}