{
  "name": "easyprint",
  "displayName": "easyprint",
  "description": "An extension to easily add debugging print statements to your code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "easyprint.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "easyprint.keybindingHover",
        "title": "Keybinding"
      }
    ],
    "keybindings": [
      {
        "command": "easyprint.keybindingHover",
        "key": "ctrl+alt+l",
        "mac": "shift+cmd+f"
      },
      {
        "command": "easyprint.keybindingHighlight",
        "key": "ctrl+alt+h",
        "mac": "cmd+shift+h",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile",
    "test": "jest"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
